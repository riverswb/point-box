<!DOCTYPE html>
<html>
  <head>
    <title>PointBox</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |key, value| %>
        <%= content_tag :div, value, class: "flash_#{key}" %>
    <% end %>
    <%= yield %>
  </body>
</html>
